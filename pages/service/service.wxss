.slide-image{
  width: 100%;
}

.swiper{
  z-index: 1;
}

.navbar{
  width: 100%;
  display: flex;
  flex-flow: wrap;
  padding: 10px 0;
  background-color: #ffffff;
  border-top-right-radius: 20px;
  border-top-left-radius: 20px;
  z-index: 2
}

.navbar-item{
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 10px 0;
}

.navbar-img{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.content{
  margin-top: 20rpx
}